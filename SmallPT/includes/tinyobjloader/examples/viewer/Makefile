# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release_native
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release_native)
  objview_config = release_native
endif
ifeq ($(config),release_x64)
  objview_config = release_x64
endif
ifeq ($(config),release_x32)
  objview_config = release_x32
endif
ifeq ($(config),debug_native)
  objview_config = debug_native
endif
ifeq ($(config),debug_x64)
  objview_config = debug_x64
endif
ifeq ($(config),debug_x32)
  objview_config = debug_x32
endif

PROJECTS := objview

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

objview:
ifneq (,$(objview_config))
	@echo "==== Building objview ($(objview_config)) ===="
	@${MAKE} --no-print-directory -C . -f objview.make config=$(objview_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f objview.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release_native"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo "  debug_native"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   objview"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"